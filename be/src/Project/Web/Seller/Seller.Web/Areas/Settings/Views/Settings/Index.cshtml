<div id="root" asp-prerender-module="SettingsPage" asp-prerender-data="@Model">
</div>

<script>
    window.data = @Html.Raw(Json.Serialize(@Model));
</script>

@section MetaTags {
    <title>@MetaTagsService.GenerateTitle(this.Localizer.GetString("Settings"))</title>
    @if (!string.IsNullOrWhiteSpace(this.Localizer.GetString("Settings")))
    {
        <meta name="description" content="@this.Localizer.GetString("Settings")" />
    }
}

@section HeadStyles {
    <link rel="stylesheet" href="~/dist/css/settingspage.css" type="text/css" asp-append-version="true" />
}

@section BodyScripts {
    <script src="~/dist/js/settingspage.js" asp-append-version="true"></script>
}