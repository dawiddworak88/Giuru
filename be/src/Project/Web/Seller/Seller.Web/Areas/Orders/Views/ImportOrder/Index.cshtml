<div id="root" asp-prerender-module="ImportOrderPage" asp-prerender-data="@Model">
</div>

<script>
    window.data = @Html.Raw(Json.Serialize(@Model));
</script>

@section MetaTags {
    <title>@MetaTagsService.GenerateTitle(this.orderLocalizer.GetString("ImportOrder"))</title>
    @if (!string.IsNullOrWhiteSpace(this.orderLocalizer.GetString("ImportOrder")))
    {
        <meta name="description" content="@this.orderLocalizer.GetString("ImportOrder")" />
    }
}

@section HeadStyles {
    <link rel="stylesheet" href="~/dist/css/importorderpage.css" type="text/css" asp-append-version="true" />
}

@section BodyScripts {
    <script src="~/dist/js/importorderpage.js" asp-append-version="true"></script>
}