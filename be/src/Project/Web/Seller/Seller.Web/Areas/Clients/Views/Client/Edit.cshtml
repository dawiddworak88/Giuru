<div id="root" asp-prerender-module="ClientPage" asp-prerender-data="@Model">
</div>

<script>
    window.data = @Html.Raw(Json.Serialize(@Model));
</script>

@section MetaTags {
    <title>@MetaTagsService.GenerateTitle(this.clientLocalizer.GetString("Client"))</title>
    @if (!string.IsNullOrWhiteSpace(this.clientLocalizer.GetString("Client")))
    {
        <meta name="description" content="@this.clientLocalizer.GetString("Client")" />
    }
}

@section HeadStyles {
    <link rel="stylesheet" href="~/dist/css/clientpage.css" type="text/css" asp-append-version="true" />
}

@section BodyScripts {
    <script src="~/dist/js/clientpage.js" asp-append-version="true"></script>
}