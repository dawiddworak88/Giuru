<div id="root" asp-prerender-module="ClientAddressPage" asp-prerender-data="@Model">
</div>

<script>
    window.data = @Html.Raw(Json.Serialize(@Model));
</script>

@section MetaTags {
    <title>@MetaTagsService.GenerateTitle(this.globalLocalizer.GetString("EditClientAddress"))</title>
    @if (!string.IsNullOrWhiteSpace(this.globalLocalizer.GetString("EditClientAddress")))
    {
        <meta name="description" content="@this.globalLocalizer.GetString("EditClientAddress")" />
    }
}

@section HeadStyles {
    <link rel="stylesheet" href="~/dist/css/clientaddresspage.css" type="text/css" asp-append-version="true" />
}

@section BodyScripts {
    <script src="~/dist/js/clientaddresspage.js" asp-append-version="true"></script>
}