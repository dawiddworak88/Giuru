<div id="root" asp-prerender-module="ClientAddressesPage" asp-prerender-data="@Model">
</div>

<script>
    window.data = @Html.Raw(Json.Serialize(@Model));
</script>

@section MetaTags {
    <title>@MetaTagsService.GenerateTitle(this.globalLocalizer.GetString("ClientAddresses"))</title>
    @if (!string.IsNullOrWhiteSpace(this.globalLocalizer.GetString("ClientAddresses")))
    {
        <meta name="description" content="@this.globalLocalizer.GetString("ClientAddresses")" />
    }
}

@section HeadStyles {
    <link rel="stylesheet" href="~/dist/css/clientaddressespage.css" type="text/css" asp-append-version="true" />
}

@section BodyScripts {
    <script src="~/dist/js/clientaddressespage.js" asp-append-version="true"></script>
}