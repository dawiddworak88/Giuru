<div id="root" asp-prerender-module="ClientApprovalsPage" asp-prerender-data="@Model">
</div>

<script>
    window.data = @Html.Raw(Json.Serialize(@Model));
</script>

@section MetaTags {
    <title>@MetaTagsService.GenerateTitle(this.clientLocalizer.GetString("ClientApprovals"))</title>
    @if (string.IsNullOrWhiteSpace(this.clientLocalizer.GetString("ClientApprovals")) is false)
    {
        <meta name="description" content="@this.clientLocalizer.GetString("ClientApprovals")" />
    }
}

@section HeadStyles {
    <link rel="stylesheet" href="~/dist/css/clientapprovalspage.css" type="text/css" asp-append-version="true" />
}

@section BodyScripts {
    <script src="~/dist/js/clientapprovalspage.js" asp-append-version="true"></script>
}