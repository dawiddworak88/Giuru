<div id="root" asp-prerender-module="CurrenciesPage" asp-prerender-data="@Model">
</div>

<script>
    Window.data = @Html.Raw(Json.Serialize(@Model));
</script>

@section MetaTags {
    <title>@MetaTagsService.GenerateTitle(_globalLocalizer.GetString("Currencies"))</title>
    @if (!string.IsNullOrWhiteSpace(_globalLocalizer.GetString("Currencies")))
    {
        <meta name="description" content="@_globalLocalizer.GetString("Currencies")" />
    }
}

@section HeadStyles {
    <link rel="stylesheet" href="~/dist/css/currenciespage.css" type="text/css" asp-append-version="true" />
}

@section BodyScripts {
    <script src="~/dist/js/currenciespage.js" asp-append-version="true"></script>
}