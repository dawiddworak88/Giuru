<div id="root" asp-prerender-module="CategoryPage" asp-prerender-data="@Model">
</div>

<script>
    window.data = @Html.Raw(Json.Serialize(@Model));
</script>

@section MetaTags {
    @if (!string.IsNullOrWhiteSpace(((CategoryPageViewModel)Model)?.Catalog?.Title))
    {
        <title>@MetaTagsService.GenerateTitle($"{((CategoryPageViewModel)Model)?.Catalog?.Title}", true)</title>
    }

    @if (!string.IsNullOrWhiteSpace(((CategoryPageViewModel)Model)?.Catalog?.Title))
    {
        <meta name="description" content="@(((CategoryPageViewModel)Model)?.Catalog?.Title)" />
    }
}

@section HeadStyles {
    <link rel="stylesheet" href="~/dist/css/categorypage.css" type="text/css" asp-append-version="true" />
}

@section BodyScripts {
    <script src="~/dist/js/categorypage.js" asp-append-version="true"></script>
}