<div id="root" asp-prerender-module="BuyerBasketPage" asp-prerender-data="@Model">
</div>

<script>
    window.data = @Html.Raw(Json.Serialize(@Model));
</script>

@section MetaTags {
    <title>@MetaTagsService.GenerateTitle(this.globalLocalizer.GetString("Basket"))</title>
    @if (!string.IsNullOrWhiteSpace(this.globalLocalizer.GetString("Basket")))
    {
        <meta name="description" content="@this.globalLocalizer.GetString("Basket")" />
    }
}

@section HeadStyles {
    <link rel="stylesheet" href="~/dist/css/basketpage.css" type="text/css" asp-append-version="true" />
}

@section BodyScripts {
    <script src="~/dist/js/basketpage.js" asp-append-version="true"></script>
}